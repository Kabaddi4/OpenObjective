<div class="container">
  <div class="row">
    <h2>フォロワー一覧</h2>
  </div>

  <div class="row">
    <% @users.each do |user| %>
    <tbody>
      <tr>
        <td><%= link_to user_path(user.id) do %>
        <%= user.name %>
        <% end %></td>
      </tr>
    </tbody>
    <% end %>
  </div>
</div>